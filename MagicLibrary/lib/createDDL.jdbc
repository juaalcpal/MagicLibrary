CREATE TABLE TITLE (ID BIGINT NOT NULL, AUTHOR VARCHAR, NAME VARCHAR, PUBLISHEDAT DATE, PRIMARY KEY (ID))
CREATE TABLE BOOK (ID BIGINT NOT NULL, AUTHOR VARCHAR, ISBN VARCHAR, NAME VARCHAR, PAGES INTEGER, PUBLISHEDAT DATE, PRIMARY KEY (ID))
CREATE TABLE AUDIORECORD (ID BIGINT NOT NULL, AUTHOR VARCHAR, DURATION INTEGER, LABEL VARCHAR, NAME VARCHAR, PUBLISHEDAT DATE, TRACKS VARCHAR, PRIMARY KEY (ID))
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT NUMERIC(38), PRIMARY KEY (SEQ_NAME))
